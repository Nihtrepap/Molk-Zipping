<Window x:Class="MolkZipping.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MolkZipping"
        mc:Ignorable="d"
        Title="MainWindow"  Height="550" Width="800" Background="White" Foreground="White">

    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styling/StyleSheet.xaml"/>
                <ResourceDictionary Source="Styling/BlendStyleSheet.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <Storyboard x:Key="LoadingBalls">
                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(RotateTransform.Angle)" Storyboard.TargetName="image">
                    <EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="3.499"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="0.051"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:0.4" Value="-1.934"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="-0.053"/>
                </DoubleAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)" Storyboard.TargetName="BallOne">
                    <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="57"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:0.6" Value="56"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:0.8" Value="-10"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1" Value="-80"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1.2" Value="-138"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1.4" Value="-142"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1.6" Value="-113.333"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1.8" Value="-36"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:2" Value="51.333"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:2.2" Value="56.666"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:2.3" Value="0.666"/>
                </DoubleAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)" Storyboard.TargetName="BallOne">
                    <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="0.5"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:0.6" Value="107"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:0.8" Value="207"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1" Value="192.333"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1.2" Value="106.333"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1.4" Value="-41.001"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1.6" Value="-131.668"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1.8" Value="-145.001"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:2" Value="-121.668"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:2.2" Value="0.332"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:2.3" Value="-3.001"/>
                </DoubleAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)" Storyboard.TargetName="BallTwo">
                    <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="1"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="56.5"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:0.6" Value="55"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:0.8" Value="-35.667"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1" Value="-104.334"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1.2" Value="-143.667"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1.4" Value="-142.333"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1.6" Value="-88.333"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1.8" Value="-10.333"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:2" Value="52.334"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:2.2" Value="56.334"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:2.3" Value="1.001"/>
                </DoubleAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)" Storyboard.TargetName="BallTwo">
                    <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="0"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="2"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:0.6" Value="107.5"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:0.8" Value="180.167"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1" Value="159.5"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1.2" Value="54.833"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1.4" Value="-93.833"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1.6" Value="-167.833"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1.8" Value="-169.833"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:2" Value="-120.5"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:2.2" Value="2.833"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:2.3" Value="-1.167"/>
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>

        </ResourceDictionary>
    </Window.Resources>

    <Window.Triggers>
        <EventTrigger RoutedEvent="UIElement.MouseEnter" SourceName="Instagram">
            <BeginStoryboard x:Name="InstagramIcon_BeginStoryboard" Storyboard="{StaticResource InstagramIcon}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseLeave" SourceName="Instagram">
            <RemoveStoryboard BeginStoryboardName="InstagramIcon_BeginStoryboard"/>
            <RemoveStoryboard BeginStoryboardName="InstagramIcon_BeginStoryboard"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseLeave" SourceName="Facebook">
            <RemoveStoryboard BeginStoryboardName="FacebookIcon_BeginStoryboard"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseEnter" SourceName="Facebook">
            <BeginStoryboard x:Name="FacebookIcon_BeginStoryboard" Storyboard="{StaticResource FacebookIcon}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseEnter" SourceName="Web">
            <BeginStoryboard x:Name="Webicon_BeginStoryboard" Storyboard="{StaticResource Webicon}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseLeave" SourceName="Web">
            <RemoveStoryboard BeginStoryboardName="Webicon_BeginStoryboard"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseEnter" SourceName="BtnPack">
            <BeginStoryboard x:Name="BtnPack_BeginStoryboard" Storyboard="{StaticResource BtnPack}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseLeave" SourceName="BtnPack">
            <RemoveStoryboard BeginStoryboardName="BtnPack_BeginStoryboard"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseEnter" SourceName="BtnUnpack">
            <BeginStoryboard x:Name="BtnUnpack_BeginStoryboard" Storyboard="{StaticResource BtnUnpack}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseLeave" SourceName="BtnUnpack">
            <RemoveStoryboard BeginStoryboardName="BtnUnpack_BeginStoryboard"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseEnter" SourceName="question">
            <BeginStoryboard x:Name="BtnQuestion_BeginStoryboard" Storyboard="{StaticResource BtnQuestion}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseLeave" SourceName="question">
            <RemoveStoryboard BeginStoryboardName="BtnQuestion_BeginStoryboard"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseEnter" SourceName="Exit">
            <BeginStoryboard x:Name="BtnExit_BeginStoryboard" Storyboard="{StaticResource BtnExit}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseLeave" SourceName="Exit">
            <RemoveStoryboard BeginStoryboardName="BtnExit_BeginStoryboard"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseEnter" SourceName="BtnBackPack">
            <BeginStoryboard x:Name="BtnPackBack_BeginStoryboard" Storyboard="{StaticResource BtnPackBack}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseLeave" SourceName="BtnBackPack">
            <RemoveStoryboard BeginStoryboardName="BtnPackBack_BeginStoryboard"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseEnter" SourceName="BtnBackUnPack">
            <BeginStoryboard x:Name="BtnBackUnpack_BeginStoryboard" Storyboard="{StaticResource BtnBackUnpack}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseLeave" SourceName="BtnBackUnPack">
            <RemoveStoryboard BeginStoryboardName="BtnBackUnpack_BeginStoryboard"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="FrameworkElement.Loaded">
            <BeginStoryboard x:Name="LogoStart_BeginStoryboard" Storyboard="{StaticResource LogoStart}"/>
            <BeginStoryboard Storyboard="{StaticResource LoadingBalls}"/>
        </EventTrigger>
    </Window.Triggers>

    <Grid>
        <Grid Name="Main" Visibility="Hidden">

            <Grid.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="White" Offset="0.651"/>
                </LinearGradientBrush>
            </Grid.Background>

            <Button Click="Button_Click" Cursor="Hand" x:Name="BtnUnpack"  Style="{StaticResource RadiusButton}" VerticalAlignment="Top" Height="122" Background="#403273" Margin="492,214,100,0" RenderTransformOrigin="0.5,0.5">
                <Button.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Button.RenderTransform>
                <Image Source="Assets/OrientationIcons/unpack.png" Width="88"/>
            </Button>
            <Button Click="Button_Click" Cursor="Hand" x:Name="BtnPack"  Style="{StaticResource RadiusButton}" VerticalAlignment="Top" Height="122" Background="#403273" Margin="100,214,492,0" RenderTransformOrigin="0.5,0.5">
                <Button.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Button.RenderTransform>
                <Image Source="Assets/OrientationIcons/Pack.png" Width="88"/>
            </Button>
            <Image MouseDown="Button_Click" Cursor="Hand" x:Name="question" Source="Assets/OrientationIcons/question.png" HorizontalAlignment="Left" Height="64" Margin="365,407,0,0" VerticalAlignment="Top" Width="64" RenderTransformOrigin="0.5,0.5">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>

            </Image>

            <Image x:Name="Exit" Cursor="Hand" Source="Assets/OrientationIcons/exit.png" HorizontalAlignment="Left" Height="32" Margin="750,10,0,0" VerticalAlignment="Top" Width="32" OpacityMask="White" RenderTransformOrigin="0.5,0.5" MouseDown="Button_Click">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>

            </Image>

            <Image Source="Assets/Images/molk.png" HorizontalAlignment="Left" Height="100" Margin="298,34,0,0" VerticalAlignment="Top" Width="200"/>

            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="WECLOME TO" VerticalAlignment="Top" Margin="157,75,0,0" RenderTransformOrigin="0.5,0.5" Width="78" Foreground="#FFC0BCC4">
                <TextBlock.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="-0.396"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </TextBlock.RenderTransform>
            </TextBlock>
            <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="809,53,-231,0"  VerticalAlignment="Top" Height="81" Width="214" Foreground="#FF251240" Text="ZIPPING" FontFamily="Tahoma" FontSize="48" FontWeight="Thin" Padding="0" RenderTransformOrigin="0.5,0.5">
                <TextBlock.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </TextBlock.RenderTransform>
                <TextBlock.Effect>
                    <DropShadowEffect ShadowDepth="1" Color="White" BlurRadius="2"/>
                </TextBlock.Effect>
            </TextBlock>
            <TextBlock x:Name="PackText" HorizontalAlignment="Left" Margin="326,241,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FF403273" FontFamily="Microsoft New Tai Lue" FontWeight="Bold" FontSize="24" Height="70" Visibility="Visible" Opacity="0" Width="141"><Run Text="   "/><Run Text="Press to"/><LineBreak/><Run Text=" "/><Run Text="  "/><Run Text="pack files"/></TextBlock>
            <TextBlock x:Name="UnpackText" HorizontalAlignment="Left" Margin="326,241,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FF403273" FontFamily="Microsoft New Tai Lue" FontWeight="Bold" FontSize="24" Height="74" Visibility="Visible" Opacity="0"><Run Text="   "/><Run Text="Press to"/><LineBreak/><Run Text=" unpack files"/></TextBlock>

            <TextBlock x:Name="HelpText" Margin="298,357,294,117" TextWrapping="Wrap" Width="200" Foreground="#FFD1521B" FontFamily="Microsoft New Tai Lue" FontSize="20" Opacity="0" FontWeight="Bold"  ><Run Text="       "/><Run Text="Press for help!"/></TextBlock>
            <TextBlock x:Name="ExitText" HorizontalAlignment="Left" Margin="753,47,0,0" TextWrapping="Wrap" Text="Exit" VerticalAlignment="Top" FontFamily="Tahoma" Foreground="#FFD1521B" FontSize="16" Opacity="0"/>
            <Button Click="Button_Click" Cursor="Hand" x:Name="BtnMenu" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="28" Height="28" BorderThickness="0" BorderBrush="{x:Null}" Background="{x:Null}" Foreground="{x:Null}"  >
                <Image Source="Assets/OrientationIcons/menu.png" Width="28" Height="28"/>
            </Button>

        </Grid>
        <Grid Name="Pack" Visibility="Hidden">
            <Button Cursor="Hand" x:Name="BtnPackFiles"  Style="{StaticResource RadiusButton}" 
                VerticalAlignment="Top" Height="122" 
                Background="#403273" Margin="662,387,10,0"
                    >
                <Image Source="Assets/OrientationIcons/pack.png" Width="88"/>
            </Button>
            <Button Cursor="Hand" x:Name="BtnChoosePackFile" Style="{StaticResource RadiusButton}" 
                VerticalAlignment="Top" Height="122" 
                Background="#403273" Margin="527,387,135,0" >
                <Image Source="Assets/OrientationIcons/chooseFile.png" Width="88" />
            </Button>
            <TextBlock HorizontalAlignment="Left" Margin="130,22,0,0"  
                   VerticalAlignment="Top" Height="81" 
                   Width="214" Foreground="#FF251240" 
                   Text="PACK FILES" FontFamily="Tahoma"
                   FontSize="38" FontWeight="Thin" 
                   Padding="0"/>

            <Button Cursor="Hand" x:Name="BtnBackPack" HorizontalAlignment="Left" 
               Height="80" Margin="25,409,0,0" 
               VerticalAlignment="Top" Width="80"
               Click="Button_Click"
                    Style="{StaticResource RadiusButton}"
                    >
                <Image Source="Assets/OrientationIcons/backBtn.png" Width="68" OpacityMask="Black"/>
            </Button>
            <Image Source="Assets/Images/molk.png" HorizontalAlignment="Left" 
               Height="100" Margin="25,20,0,0" 
               VerticalAlignment="Top" Width="100"/>
            <DataGrid x:Name="GridPack" HorizontalAlignment="Left" Height="304" VerticalAlignment="Top" Width="502" Margin="25,78,0,0"/>

        </Grid>
        <Grid Name="Unpack" Visibility="Hidden">
            <Button Cursor="Hand" x:Name="BtnUnPackFiles"  Style="{StaticResource RadiusButton}" 
                VerticalAlignment="Top" Height="122" 
                Background="#403273" Margin="662,387,10,0">
                <Image Source="Assets/OrientationIcons/unpack.png" Width="88"/>
            </Button>
            <Button Cursor="Hand" x:Name="BtnChooseUnpackFiles" Style="{StaticResource RadiusButton}" 
                VerticalAlignment="Top" Height="122" 
                Background="#403273" Margin="527,387,135,0">
                <Image Source="Assets/OrientationIcons/chooseFile.png" Width="88"/>
            </Button>
            <TextBlock HorizontalAlignment="Left" Margin="130,22,0,0"  
                   VerticalAlignment="Top" Height="81" 
                   Width="287" Foreground="#FF251240" 
                   Text="UNPACK FILES" FontFamily="Tahoma"
                   FontSize="38" FontWeight="Thin" 
                   Padding="0"/>
            <Button Cursor="Hand" x:Name="BtnBackUnPack" HorizontalAlignment="Left" 
               Height="80" Margin="25,409,0,0"
               VerticalAlignment="Top" Width="80"
               Click="Button_Click" 
                    Style="{StaticResource RadiusButton}">
                <Image Source="Assets/OrientationIcons/backBtn.png" Width="68" OpacityMask="Black"/>
            </Button>
            <Image Source="Assets/Images/molk.png" HorizontalAlignment="Left" 
               Height="100" Margin="25,20,0,0" 
               VerticalAlignment="Top" Width="100"/>
            <DataGrid x:Name="GridUnpack" HorizontalAlignment="Left" Height="304" VerticalAlignment="Top" Width="502" Margin="25,78,0,0"/>

        </Grid>
        <Grid Name="Advanced" Visibility="Hidden" Margin="0,0,600,0" Background="#7F403273" MouseLeave="Mouse_Leave_Menu"  >
            <Image MouseDown="Button_Click" Cursor="Hand" x:Name="Instagram" Source="Assets/SocialIcons/instagram.png" HorizontalAlignment="Left" Height="28" Margin="10,380,0,0" VerticalAlignment="Top" Width="28" RenderTransformOrigin="0.5,0.5">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
            <Image MouseDown="Button_Click" Cursor="Hand" x:Name="Facebook" Source="Assets/SocialIcons/facebook.png" HorizontalAlignment="Left" Height="28" Margin="10,420,0,0" VerticalAlignment="Top" Width="28" RenderTransformOrigin="0.5,0.5">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
            <Image MouseDown="Button_Click" Cursor="Hand" x:Name="Web" Source="Assets/SocialIcons/web.png" HorizontalAlignment="Left" Height="28" Margin="10,460,0,0" VerticalAlignment="Top" Width="28" RenderTransformOrigin="0.5,0.5">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
        </Grid>
        <Grid Name="Loading" Visibility="Visible">
            <TextBlock x:Name="TxtLoading" Margin="249,10,249,0"  VerticalAlignment="Top" Height="131" Foreground="#FF251240" FontFamily="Tahoma" FontSize="48" FontWeight="Thin" Padding="0"><Run Text="Working files"/><Run Text=","/><Run Text=" "/><LineBreak/><Run Text="p"/><Run Text="lease wait..."/></TextBlock>

            <Image x:Name="image" Source="Assets/Images/molkLoading.png" Margin="269,192,269,191" RenderTransformOrigin="0.5,0.5">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
            <Image x:Name="BallOne" Source="Assets/Images/loadingOne.png" Margin="368,289,393,203" RenderTransformOrigin="0.5,0.5">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
            <Image x:Name="BallTwo" Source="Assets/Images/loadingTwo.png" Margin="394,289,368,203" RenderTransformOrigin="0.5,0.5">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
        </Grid>
    </Grid>
</Window>
